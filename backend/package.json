{
  "name": "leisure-club-backend",
  "version": "1.0.0",
  "description": "Backend API for Leisure Club Management System",
  "type": "commonjs",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:api": "node test-api.js",
    "test:api:quick": "node quick-test.js",
    "test:connectivity": "node test-connectivity.js",
    "db:migrate": "node -r dotenv/config src/database/migrate.js",
    "db:migrate:safe": "node src/database/safe-migrate.js",
    "db:seed": "node -r dotenv/config src/database/seed.js"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.29.4",
    "nodemailer": "^6.9.7",
    "pg": "^8.11.3",
    "twilio": "^4.19.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "eslint": "^9.38.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3"
  },
  "keywords": [
    "leisure",
    "club",
    "booking",
    "membership",
    "api"
  ],
  "author": "Leisure Club Development Team",
  "license": "MIT"
}
